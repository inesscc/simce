{
    "name": "maxvit_t",
    "n_gpu": 4,
    "class_weights": [
        1.0,
        1.0
    ],
    "arch": {
        "type": "maxvit_t",
        "args": {}
    },
    "weights": {
        "type": "MaxVit_T_Weights",
        "args": {}
    },
    "data_loader": {
        "type": "TrainTestDataLoader",
        "args": {
            "data_file": "data_pred.csv",
            "batch_size": 64,
            "shuffle": false,
            "cortar_bordes": false,
            "num_workers": 4,

            "validation_split": 0.0
        }
    },

    "optimizer": {
        "type": "SGD",
        "args": {
            "lr": 0.005,
            "weight_decay": 0.001,
            "momentum": 0.9
        }
    },
    "loss": {
        "type": "CrossEntropyLoss",
        "args": {}
    },
    "metrics": [
        "accuracy"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 2,
            "gamma": 0.8
        }
    },
    "trainer": {
        "epochs": 100,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": false
    },

    "directorios": {
        "type": "get_directorios",
        "args": {}
    }
}